{
  "name": "Python CI Testing",
  "dockerComposeFile": "compose.yaml",
  "service": "proj",
  "workspaceFolder": "/workspaces",
  "features": {
    "ghcr.io/eitsupi/devcontainer-features/go-task:1": {},
    "ghcr.io/devcontainers/features/node:1": {}
